<?xml version="1.0"?>
<xqx:module xmlns:xqx="http://www.w3.org/2005/XQueryX"
            xmlns:xqxuf="http://www.w3.org/2007/xquery-update-10"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.w3.org/2007/xquery-update-10
                                http://www.w3.org/2007/xquery-update-10/xquery-update-10-xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:copyNamespacesDecl>
        <xqx:preserveMode>preserve</xqx:preserveMode>
        <xqx:inheritMode>no-inherit</xqx:inheritMode>
      </xqx:copyNamespacesDecl>
      <xqx:varDecl>
        <xqx:varName>input-context</xqx:varName>
        <xqx:external/>
      </xqx:varDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:flworExpr>
        <xqx:letClause>
          <xqx:letClauseItem>
            <xqx:typedVariableBinding>
              <xqx:varName>a</xqx:varName>
            </xqx:typedVariableBinding>
            <xqx:letExpr>
              <xqx:elementConstructor>
                <xqx:tagName xqx:prefix="x">a</xqx:tagName>
                <xqx:attributeList>
                  <xqx:namespaceDeclaration>
                    <xqx:prefix>x</xqx:prefix>
                    <xqx:uri>http://www.example.com/a</xqx:uri>
                  </xqx:namespaceDeclaration>
                </xqx:attributeList>
                <xqx:elementContent>
                  <xqx:elementConstructor>
                    <xqx:tagName>b</xqx:tagName>
                  </xqx:elementConstructor>
                </xqx:elementContent>
              </xqx:elementConstructor>
            </xqx:letExpr>
          </xqx:letClauseItem>
        </xqx:letClause>
        <xqx:letClause>
          <xqx:letClauseItem>
            <xqx:typedVariableBinding>
              <xqx:varName>copyb</xqx:varName>
            </xqx:typedVariableBinding>
            <xqx:letExpr>
              <xqxuf:transformExpr>
                <xqxuf:transformCopies>
                  <xqxuf:transformCopy>
                    <xqx:varRef>
                      <xqx:name>b</xqx:name>
                    </xqx:varRef>
                    <xqxuf:copySource>
                      <xqx:pathExpr>
                        <xqx:stepExpr>
                          <xqx:filterExpr>
                            <xqx:varRef>
                              <xqx:name>a</xqx:name>
                            </xqx:varRef>
                          </xqx:filterExpr>
                        </xqx:stepExpr>
                        <xqx:stepExpr>
                          <xqx:xpathAxis>child</xqx:xpathAxis>
                          <xqx:nameTest>b</xqx:nameTest>
                        </xqx:stepExpr>
                      </xqx:pathExpr>
                    </xqxuf:copySource>
                  </xqxuf:transformCopy>
                </xqxuf:transformCopies>
                <xqxuf:modifyExpr>
                  <xqx:sequenceExpr/>
                </xqxuf:modifyExpr>
                <xqxuf:returnExpr>
                  <xqx:varRef>
                    <xqx:name>b</xqx:name>
                  </xqx:varRef>
                </xqxuf:returnExpr>
              </xqxuf:transformExpr>
            </xqx:letExpr>
          </xqx:letClauseItem>
        </xqx:letClause>
        <xqx:returnClause>
          <xqx:functionCallExpr>
            <xqx:functionName>empty</xqx:functionName>
            <xqx:arguments>
              <xqx:pathExpr>
                <xqx:stepExpr>
                  <xqx:filterExpr>
                    <xqx:functionCallExpr>
                      <xqx:functionName>in-scope-prefixes</xqx:functionName>
                      <xqx:arguments>
                        <xqx:varRef>
                          <xqx:name>copyb</xqx:name>
                        </xqx:varRef>
                      </xqx:arguments>
                    </xqx:functionCallExpr>
                  </xqx:filterExpr>
                  <xqx:predicates>
                    <xqx:equalOp>
                      <xqx:firstOperand>
                        <xqx:contextItemExpr/>
                      </xqx:firstOperand>
                      <xqx:secondOperand>
                        <xqx:stringConstantExpr>
                          <xqx:value>x</xqx:value>
                        </xqx:stringConstantExpr>
                      </xqx:secondOperand>
                    </xqx:equalOp>
                  </xqx:predicates>
                </xqx:stepExpr>
              </xqx:pathExpr>
            </xqx:arguments>
          </xqx:functionCallExpr>
        </xqx:returnClause>
      </xqx:flworExpr>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
