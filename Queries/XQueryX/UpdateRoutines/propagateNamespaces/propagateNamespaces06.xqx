<?xml version="1.0"?>
<xqx:module xmlns:xqx="http://www.w3.org/2005/XQueryX"
            xmlns:xqxuf="http://www.w3.org/2007/xquery-update-10"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.w3.org/2007/xquery-update-10
                                http://www.w3.org/2007/xquery-update-10/xquery-update-10-xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:copyNamespacesDecl>
        <xqx:preserveMode>preserve</xqx:preserveMode>
        <xqx:inheritMode>no-inherit</xqx:inheritMode>
      </xqx:copyNamespacesDecl>
      <xqx:defaultNamespaceDecl>
        <xqx:defaultNamespaceCategory>element</xqx:defaultNamespaceCategory>
        <xqx:uri>bar</xqx:uri>
      </xqx:defaultNamespaceDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqxuf:transformExpr>
        <xqxuf:transformCopies>
          <xqxuf:transformCopy>
            <xqx:varRef>
              <xqx:name>x</xqx:name>
            </xqx:varRef>
            <xqxuf:copySource>
              <xqx:elementConstructor>
                <xqx:tagName>x</xqx:tagName>
                <xqx:attributeList>
                  <xqx:namespaceDeclaration>
                    <xqx:uri>foo</xqx:uri>
                  </xqx:namespaceDeclaration>
                  <xqx:namespaceDeclaration>
                    <xqx:prefix>a</xqx:prefix>
                    <xqx:uri>a-ns</xqx:uri>
                  </xqx:namespaceDeclaration>
                </xqx:attributeList>
              </xqx:elementConstructor>
            </xqxuf:copySource>
          </xqxuf:transformCopy>
        </xqxuf:transformCopies>
        <xqxuf:modifyExpr>
          <xqxuf:insertExpr>
            <xqxuf:sourceExpr>
              <xqx:elementConstructor>
                <xqx:tagName>y</xqx:tagName>
              </xqx:elementConstructor>
            </xqxuf:sourceExpr>
            <xqxuf:insertInto/>
            <xqxuf:targetExpr>
              <xqx:varRef>
                <xqx:name>x</xqx:name>
              </xqx:varRef>
            </xqxuf:targetExpr>
          </xqxuf:insertExpr>
        </xqxuf:modifyExpr>
        <xqxuf:returnExpr>
          <xqx:elementConstructor>
            <xqx:tagName>result</xqx:tagName>
            <xqx:elementContent>
              <xqx:stringConstantExpr>
                <xqx:value>
  </xqx:value>
              </xqx:stringConstantExpr>
              <xqx:elementConstructor>
                <xqx:tagName>x</xqx:tagName>
                <xqx:elementContent>
                  <xqx:sequenceExpr>
                    <xqx:functionCallExpr>
                      <xqx:functionName>namespace-uri-for-prefix</xqx:functionName>
                      <xqx:arguments>
                        <xqx:stringConstantExpr>
                          <xqx:value></xqx:value>
                        </xqx:stringConstantExpr>
                        <xqx:varRef>
                          <xqx:name>x</xqx:name>
                        </xqx:varRef>
                      </xqx:arguments>
                    </xqx:functionCallExpr>
                    <xqx:functionCallExpr>
                      <xqx:functionName>namespace-uri-for-prefix</xqx:functionName>
                      <xqx:arguments>
                        <xqx:stringConstantExpr>
                          <xqx:value>a</xqx:value>
                        </xqx:stringConstantExpr>
                        <xqx:varRef>
                          <xqx:name>x</xqx:name>
                        </xqx:varRef>
                      </xqx:arguments>
                    </xqx:functionCallExpr>
                  </xqx:sequenceExpr>
                </xqx:elementContent>
              </xqx:elementConstructor>
              <xqx:stringConstantExpr>
                <xqx:value>
  </xqx:value>
              </xqx:stringConstantExpr>
              <xqx:elementConstructor>
                <xqx:tagName>y</xqx:tagName>
                <xqx:elementContent>
                  <xqx:sequenceExpr>
                    <xqx:functionCallExpr>
                      <xqx:functionName>namespace-uri-for-prefix</xqx:functionName>
                      <xqx:arguments>
                        <xqx:stringConstantExpr>
                          <xqx:value></xqx:value>
                        </xqx:stringConstantExpr>
                        <xqx:pathExpr>
                          <xqx:stepExpr>
                            <xqx:filterExpr>
                              <xqx:varRef>
                                <xqx:name>x</xqx:name>
                              </xqx:varRef>
                            </xqx:filterExpr>
                          </xqx:stepExpr>
                          <xqx:stepExpr>
                            <xqx:xpathAxis>child</xqx:xpathAxis>
                            <xqx:nameTest>y</xqx:nameTest>
                          </xqx:stepExpr>
                        </xqx:pathExpr>
                      </xqx:arguments>
                    </xqx:functionCallExpr>
                    <xqx:functionCallExpr>
                      <xqx:functionName>namespace-uri-for-prefix</xqx:functionName>
                      <xqx:arguments>
                        <xqx:stringConstantExpr>
                          <xqx:value>a</xqx:value>
                        </xqx:stringConstantExpr>
                        <xqx:pathExpr>
                          <xqx:stepExpr>
                            <xqx:filterExpr>
                              <xqx:varRef>
                                <xqx:name>x</xqx:name>
                              </xqx:varRef>
                            </xqx:filterExpr>
                          </xqx:stepExpr>
                          <xqx:stepExpr>
                            <xqx:xpathAxis>child</xqx:xpathAxis>
                            <xqx:nameTest>y</xqx:nameTest>
                          </xqx:stepExpr>
                        </xqx:pathExpr>
                      </xqx:arguments>
                    </xqx:functionCallExpr>
                  </xqx:sequenceExpr>
                </xqx:elementContent>
              </xqx:elementConstructor>
              <xqx:stringConstantExpr>
                <xqx:value>
</xqx:value>
              </xqx:stringConstantExpr>
            </xqx:elementContent>
          </xqx:elementConstructor>
        </xqxuf:returnExpr>
      </xqxuf:transformExpr>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
