<?xml version="1.0"?>
<xqx:module xmlns:xqx="http://www.w3.org/2005/XQueryX"
            xmlns:xqxuf="http://www.w3.org/2007/xquery-update-10"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.w3.org/2005/XQueryX
                                http://www.w3.org/2005/XQueryX/xqueryx.xsd
                                http://www.w3.org/2007/xquery-update-10
                                http://www.w3.org/2007/xquery-update-10/xquery-update-10-xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:varDecl>
        <xqx:varName>part-tree</xqx:varName>
        <xqx:external/>
      </xqx:varDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:flworExpr>
        <xqx:letClause>
          <xqx:letClauseItem>
            <xqx:typedVariableBinding>
              <xqx:varName>next</xqx:varName>
            </xqx:typedVariableBinding>
            <xqx:letExpr>
              <xqx:addOp>
                <xqx:firstOperand>
                  <xqx:functionCallExpr>
                    <xqx:functionName>max</xqx:functionName>
                    <xqx:arguments>
                      <xqx:pathExpr>
                        <xqx:stepExpr>
                          <xqx:filterExpr>
                            <xqx:varRef>
                              <xqx:name>part-tree</xqx:name>
                            </xqx:varRef>
                          </xqx:filterExpr>
                        </xqx:stepExpr>
                        <xqx:stepExpr>
                          <xqx:xpathAxis>descendant-or-self</xqx:xpathAxis>
                          <xqx:anyKindTest/>
                        </xqx:stepExpr>
                        <xqx:stepExpr>
                          <xqx:xpathAxis>attribute</xqx:xpathAxis>
                          <xqx:nameTest>partid</xqx:nameTest>
                        </xqx:stepExpr>
                      </xqx:pathExpr>
                    </xqx:arguments>
                  </xqx:functionCallExpr>
                </xqx:firstOperand>
                <xqx:secondOperand>
                  <xqx:integerConstantExpr>
                    <xqx:value>1</xqx:value>
                  </xqx:integerConstantExpr>
                </xqx:secondOperand>
              </xqx:addOp>
            </xqx:letExpr>
          </xqx:letClauseItem>
        </xqx:letClause>
        <xqx:returnClause>
          <xqxuf:insertExpr>
            <xqxuf:sourceExpr>
              <xqx:elementConstructor>
                <xqx:tagName>part</xqx:tagName>
                <xqx:attributeList>
                  <xqx:attributeConstructor>
                    <xqx:attributeName>partid</xqx:attributeName>
                    <xqx:attributeValueExpr>
                      <xqx:varRef>
                        <xqx:name>next</xqx:name>
                      </xqx:varRef>
                    </xqx:attributeValueExpr>
                  </xqx:attributeConstructor>
                  <xqx:attributeConstructor>
                    <xqx:attributeName>name</xqx:attributeName>
                    <xqx:attributeValue>radio</xqx:attributeValue>
                  </xqx:attributeConstructor>
                </xqx:attributeList>
              </xqx:elementConstructor>
            </xqxuf:sourceExpr>
            <xqxuf:insertInto>
              <xqxuf:insertAsLast/>
            </xqxuf:insertInto>
            <xqxuf:targetExpr>
              <xqx:pathExpr>
                <xqx:stepExpr>
                  <xqx:filterExpr>
                    <xqx:varRef>
                      <xqx:name>part-tree</xqx:name>
                    </xqx:varRef>
                  </xqx:filterExpr>
                </xqx:stepExpr>
                <xqx:stepExpr>
                  <xqx:xpathAxis>descendant-or-self</xqx:xpathAxis>
                  <xqx:anyKindTest/>
                </xqx:stepExpr>
                <xqx:stepExpr>
                  <xqx:xpathAxis>child</xqx:xpathAxis>
                  <xqx:nameTest>part</xqx:nameTest>
                  <xqx:predicates>
                    <xqx:andOp>
                      <xqx:firstOperand>
                        <xqx:equalOp>
                          <xqx:firstOperand>
                            <xqx:pathExpr>
                              <xqx:stepExpr>
                                <xqx:xpathAxis>attribute</xqx:xpathAxis>
                                <xqx:nameTest>partid</xqx:nameTest>
                              </xqx:stepExpr>
                            </xqx:pathExpr>
                          </xqx:firstOperand>
                          <xqx:secondOperand>
                            <xqx:integerConstantExpr>
                              <xqx:value>0</xqx:value>
                            </xqx:integerConstantExpr>
                          </xqx:secondOperand>
                        </xqx:equalOp>
                      </xqx:firstOperand>
                      <xqx:secondOperand>
                        <xqx:equalOp>
                          <xqx:firstOperand>
                            <xqx:pathExpr>
                              <xqx:stepExpr>
                                <xqx:xpathAxis>attribute</xqx:xpathAxis>
                                <xqx:nameTest>name</xqx:nameTest>
                              </xqx:stepExpr>
                            </xqx:pathExpr>
                          </xqx:firstOperand>
                          <xqx:secondOperand>
                            <xqx:stringConstantExpr>
                              <xqx:value>car</xqx:value>
                            </xqx:stringConstantExpr>
                          </xqx:secondOperand>
                        </xqx:equalOp>
                      </xqx:secondOperand>
                    </xqx:andOp>
                  </xqx:predicates>
                </xqx:stepExpr>
              </xqx:pathExpr>
            </xqxuf:targetExpr>
          </xqxuf:insertExpr>
        </xqx:returnClause>
      </xqx:flworExpr>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
